{
  "name": "amazon-connect-playground",
  "version": "0.1.0",
  "bin": {
    "amazon-connect-playground": "bin/amazon-connect-playground.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "cdk": "cdk",
    "lint": "run-s lint:* -c",
    "lint:eslint": "eslint --ext .ts --max-warnings 0 .",
    "lint:prettier": "prettier --check .",
    "lint:tsc": "tsc --pretty --noEmit",
    "format": "run-s format:* -c",
    "format:eslint": "npm run lint:eslint -- --fix",
    "format:prettier": "npm run lint:prettier -- --write"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "20.1.7",
    "aws-cdk": "2.84.0",
    "eslint-config-prettier": "^8.8.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "~5.0.4"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "aws-cdk-lib": "2.84.0",
    "constructs": "^10.0.0",
    "eslint": "^8.43.0",
    "eslint-plugin-perfectionist": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "source-map-support": "^0.5.21"
  }
}
